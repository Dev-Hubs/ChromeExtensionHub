{
  "manifest_version": 2,
  "name": "Chrome Extension Hub",
  "description": "Developer-To-Developer Hub as Tutorial to develop chrome extensions",
  "version": "1.0",
  "devtools_page": "devtools/chrome-devtools.html",
  "permissions":[
    "activeTab"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "72": "icons/72.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "browser_action":{
    "default_icon":"icons/32.png",
    "default_title":"I am the Pop Up"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ 
        "global/interface.js", 
        "content/content.js" 
        ]
    }
  ],
  "background": {
    "scripts": [
      "global/interface.js",
      "background/background.js"
    ]
  },
  "web_accessible_resources": [
    "global/interface.js",
    "inject/code.js",
    "inject/style.css"
  ],
  "minimum_chrome_version": "25"
}